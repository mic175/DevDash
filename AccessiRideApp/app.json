{
  "expo": {
    "name": "AccessiRideApp",
    "slug": "AccessiRideApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "accessirideapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.AccessiRideApp",
      "config": {
        "googleMapsApiKey": "AIzaSyBDSI8NHH8ANwyjTbgQTq2yKfW3fKmh4U4"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow AccessiRide to access your location to find your starting point."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png", 
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.AccessiRideApp",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBDSI8NHH8ANwyjTbgQTq2yKfW3fKmh4U4"
        }
      },
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
